{
    "env": {
        "browser": false,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn"
    },
    "overrides": [
        {
            "files": ["*.spec.js"],
            "rules": {
                "no-console": "off"
            }
        }
    ]
}
